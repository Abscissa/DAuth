{
	"name": "instauser-store",
	"description": "User account library",
	"authors": ["Nick Sabalausky"],
	"homepage": "https://github.com/abscissa/InstaUser",
	"license": "zlib/libpng",
	"targetType": "staticLibrary",
	"targetName": "instauser-store",
	"sourcePaths": ["dummy"],
	"importPaths": ["src"],
	"dependencies": {
		"instauser:instauser-basic": "*",
		"safearg": "~>0.9.7",
		"ddox": "~>0.10.9",
		"mysql-native": { "version": "==0.1.3", "optional": false },
		"vibe-d": { "version": "==0.7.23", "optional": false }
	},

	"postBuildCommands-posix":   ["cd $PACKAGE_DIR    && ../helper/build   store $PACKAGE_DIR $SAFEARG_PACKAGE_DIR $DDOX_PACKAGE_DIR"],
	"postBuildCommands-windows": ["cd /D $PACKAGE_DIR && ..\\helper\\build store $PACKAGE_DIR $SAFEARG_PACKAGE_DIR $DDOX_PACKAGE_DIR"],

	"configurations": [
		{
			"name": "library",
			"targetType": "library",
			"targetPath": "lib",

			"versions": ["InstaUserStore_PrebuiltLib"],
		},
		{
			"name": "tests",
			"targetType": "executable",
			"targetPath": "bin",

			"versions": ["InstaUser_AllowWeakSecurity", "InstaUserStore_Unittest", "VibeCustomMain", "VibeLibeventDriver"],
		},
		{
			"name": "docs",
			"targetType": "library",
			"targetPath": "deleteme",

			"versions": ["InstaUser_Docs"],
		}
	]
}
