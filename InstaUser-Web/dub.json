{
	"name": "instauser-web",
	"description": "Simple Web-based User Logins",
	"authors": ["Nick Sabalausky"],
	"homepage": "https://github.com/abscissa/InstaUser",
	"license": "zlib/libpng",
	"targetType": "staticLibrary",
	"targetName": "instauser-web",
	"targetPath": "deleteme",
	"sourcePaths": ["dummy"],
	"importPaths": ["src"],
	"dependencies": {
		"instauser:instauser-basic": "*",
		"instauser:instauser-store": "*",
		"safearg": "~>0.9.7",
		"ddox": "~>0.10.9",
		"semitwistweb": "==0.0.4"
	},
	"subConfigurations": {
		"semitwistweb": "no-build"
	},
	
	"preGenerateCommands":
		["rdmd $SEMITWISTWEB_PACKAGE_DIR/scripts/copyResources.d $PACKAGE_DIR $SEMITWISTWEB_PACKAGE_DIR"],
	
	"postBuildCommands-posix":   ["cd $PACKAGE_DIR    && ../helper/build   web $PACKAGE_DIR $SAFEARG_PACKAGE_DIR $DDOX_PACKAGE_DIR"],
	"postBuildCommands-windows": ["cd /D $PACKAGE_DIR && ..\\helper\\build web $PACKAGE_DIR $SAFEARG_PACKAGE_DIR $DDOX_PACKAGE_DIR"],

	"configurations": [
		{
			"name": "library",
			"targetType": "library",
			"sourcePaths": ["dummy"]
		},
		{
			"name": "tests",
			"targetType": "executable",
			"sourcePaths": ["dummy"],

			"versions": ["InstaUser_AllowWeakSecurity", "InstaUserWeb_Unittest", "VibeCustomMain", "VibeLibeventDriver"],
		},
		{
			"name": "docs",
			"targetType": "library",
			"targetPath": "deleteme",

			"versions": ["InstaUser_Docs"],
		}
	]
}
